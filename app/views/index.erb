<a name="about"></a>
<div class="intro-header">
    <div class="container">

        <div class="row">
            <div class="col-lg-12">
                <div class="intro-message">
                  <h1>All Tasks</h1>
                  <% @tasks.each do |task| %>
                    <h3><%= task.id %>: <a href="/tasks/<%= task.id %>"><%= task.title %></a></h3>
                  <% end %>
                    <hr class="intro-divider">
                </div>
            </div>
        </div>
    </div>
</div>
